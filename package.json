{
    "name": "dfdb",
    "version": "0.0.4",
    "description": "DFDB (Docs on Files DB) locally stores a very simple noSQL database.",
    "main": "lib/dfdb",
    "types": "lib/dfdb",
    "scripts": {
        "start": "npm run build:live",
        "build": "./node_modules/.bin/tsc",
        "build:live": "./node_modules/.bin/tsc -w",
        "pretest": "rm -fr ./test/.tmpdb/*",
        "test": "mkdir -p ./test/.tmpdb;./node_modules/mocha/bin/mocha"
    },
    "keywords": [
        "nosql",
        "emulated",
        "serverless",
        "on-file"
    ],
    "author": {
        "name": "Alejandro Dario Simi",
        "url": "http://daemonraco.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/daemonraco/dfdb.git"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^8.5.9",
        "chai": "^4.1.1",
        "mocha": "^3.5.0",
        "typescript": "^2.4.1"
    },
    "dependencies": {
        "@types/async": "^2.0.47",
        "@types/jszip": "^3.1.3",
        "async": "^2.6.0",
        "jsonpath-plus": "^0.16.0",
        "jszip": "^3.1.5"
    }
}